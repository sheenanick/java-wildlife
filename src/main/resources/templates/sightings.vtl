<div class="row">
  <div class="col-sm-4 col-sm-offset-4">
    <a href="/sightings/new"><button type="button" class="btn btn-primary">Add Sighting</button></a>
    <h3>All Sightings:</h3>
    #foreach($sighting in $sightings)
      <a href="/animals/$sighting.getAnimalId()"><h4>$Animal.find($sighting.getAnimalId()).getName()</h4></a>
      <h5>Location: $sighting.getLocation(), Ranger: $sighting.getRangerName()</h5>
    #end
  </div>
</div>
